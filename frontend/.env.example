# ISPMAX Frontend Environment Configuration

# API URL (absolute or relative; relative defaults to /api which proxies to backend)
VITE_API_URL=/api

# Enable/disable development features
VITE_DEBUG=false

# Backend server (used by Vite dev proxy)
VITE_BACKEND_URL=http://localhost:5000

# Enable mock API fallback only for local demo when backend is down
VITE_ENABLE_API_MOCKS=false
